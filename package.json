{
  "name": "@ryaninvents/next-lambda-tf",
  "version": "0.0.0-semantically-versioned",
  "main": "src/index.js",
  "private": true,
  "author": "Ryan Kennedy <ryaninvents@users.noreply.github.com>",
  "workspaces": [
    "packages/*"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "babel-plugin-styled-components": "^1.10.0",
    "express": "^4.16.4",
    "next": "^7.0.3",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "rebass": "^3.0.1",
    "serverless-http": "^1.9.0",
    "styled-components": "^4.1.3"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@ryaninvents/plugin-bundle-nextjs",
        {
          "nextBuildScript": [
            "build:nextjs"
          ]
        }
      ],
      [
        "@ryaninvents/plugin-bundle-dependencies",
        {
          "prefix": "nodejs/node_modules"
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "semistandard",
      "react-app"
    ],
    "overrides": [
      {
        "files": [
          "**/__tests__/**",
          "*.test.js"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 8
          },
          "modules": false
        }
      ],
      [
        "babel-preset-react-app",
        {
          "absoluteRuntime": false,
          "helpers": false
        }
      ]
    ],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ]
    ],
    "env": {
      "test": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": 8
              }
            }
          ],
          [
            "babel-preset-react-app",
            {
              "modules": true,
              "helpers": false
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true,
              "preprocess": false
            }
          ]
        ]
      },
      "development": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": 8
              }
            }
          ],
          [
            "babel-preset-react-app",
            {
              "modules": true,
              "helpers": false
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true,
              "preprocess": false
            }
          ]
        ]
      }
    }
  },
  "scripts": {
    "dev": "NODE_ENV=development babel-node src/index.js",
    "build:nextjs": "NODE_ENV=production next build",
    "build": "pack build",
    "test": "echo No tests yet",
    "start": "babel-node src/index.js"
  },
  "devDependencies": {
    "@babel/node": "^7.2.2",
    "@pika/pack": "^0.3.1",
    "@pika/plugin-build-node": "^0.3.10",
    "@pika/plugin-standard-pkg": "^0.3.10",
    "@ryaninvents/plugin-bundle-dependencies": "^1.1.1",
    "@ryaninvents/plugin-bundle-nextjs": "^1.0.0",
    "@ryaninvents/plugin-bundle-zip-node": "^1.2.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-react-app": "^7.0.1",
    "eslint": "^5.13.0",
    "eslint-config-react-app": "^3.0.7",
    "eslint-config-semistandard": "^13.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-flowtype": "^3.4.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0"
  }
}
